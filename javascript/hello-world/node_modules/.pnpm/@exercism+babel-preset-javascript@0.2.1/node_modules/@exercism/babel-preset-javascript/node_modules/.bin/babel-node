#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benv/exercism/javascript/hello-world/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benv/exercism/javascript/hello-world/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@babel+node@7.20.7_@babel+core@7.20.12/node_modules/@babel/node/bin/babel-node.js" "$@"
else
  exec node  "$basedir/../../../../../../@babel+node@7.20.7_@babel+core@7.20.12/node_modules/@babel/node/bin/babel-node.js" "$@"
fi
